

!URxvt*font: xft:xos4 Terminess Powerline:size=:antialias=true

URxvt*font: xft:Hack:size=9:antialias=true

!URxvt*font: xft:Source\ Code\ Pro\ Medium:size=9:antialias=true:hinting=true,xft:Liberation Mono:antialias=true:hinting=true

! terminal padding
!URxvt.internalBorder: -1

! terminal transparency
!URxvt.transparent:	true
!URxvt.shading:	20

URxvt*saveLines: 10000
URxvt*scrollBar: false
URxvt*letterSpace: -1
URxvt*termName: rxvt-unicode

URxvt.keysym.Shift-Control-V: perl:clipboard:paste
!URxvt.iso14755: False

URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

xterm*faceName: xft:Hack:size=9:antialias=true
!xterm*font: -*-fontawesome-medium-*-normal-*-*-*-100-100-*-*-*-*
xterm*locale: true
xterm*saveLines: 10000
xterm*scrollBar: false
xterm*termName: xterm-256color
xterm.url-select.launcher:  firefox
xterm.url-select.underline: true

